import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int t = sc.nextInt();
	//int count=0;
	for(int s = 0;s<t;s++)
	{
	 int n = sc.nextInt();
	 int[] a =new int[n];
	 for(int i = 0;i<n;i++)
	 {
	     a[i]=sc.nextInt();
	 }
	 Arrays.sort(a);
	 a[0] += 1;
	 int pro=1;
	 for(int i = 0;i<n;i++)
	 {
	     pro = pro*a[i];
	 }
	 System.out.println(pro);
	}
	
	}
}
